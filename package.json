{
  "name": "trunews",
  "version": "1.0.0",
  "description": "Ingesoft 2 unal",
  "main": "index.js",
  "scripts": {
    "test": "npm run build && jest --coverage --maxWorkers=1 --setupFiles dotenv/config",
    "testNoCoverage": "npm run build && jest --maxWorkers=1 --setupFiles dotenv/config",
    "init": "npm i && npx prisma generate",
    "singleChange": "ts-node src/app.ts",
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules src/server.ts",
    "build": "tsc",
    "start": "node build/server.js",
    "production": "tsc && node build/server.js",
    "Pm2Production": "tsc && pm2 start build/server.js --name trunews && pm2 startup && pm2 save",
    "restart": "pm2 restart my-app-name",
    "createImage": "docker build -t trunews -f DockerFile .",
    "dockerRun": "docker run -d -p 3007:3005 trunews --name trunews",
    "migratePrisma": "npx prisma migrate dev && npx prisma migrate deploy",
    "nodemon": "nodemon",
    "connectServer": "ssh -i ./trunews.pem ec2-user@ec2-35-175-244-104.compute-1.amazonaws.com",
    "poblate": "tsc && node build/utils/poblateDB",
    "trendArticle": "tsc && node build/articles/trendArticle",
    "trendUser": "tsc && node build/user/trendAuthor",
    "fullStart": "tsc && node build/utils/poblateDB && node build/articles/trendArticle && node build/user/trendAuthor"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dcanasp/trUNews.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dcanasp/trUNews/issues"
  },
  "homepage": "https://github.com/dcanasp/trUNews#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.405.0",
    "@aws-sdk/lib-storage": "^3.405.0",
    "@prisma/client": "^5.2.0",
    "argon2": "^0.31.1",
    "aws-sdk": "^2.1452.0",
    "axios": "^1.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.1",
    "fs": "^0.0.1-security",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "node-cron": "^3.0.2",
    "pm2": "^5.3.0",
    "qr-image": "^3.2.0",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.32.6",
    "tsyringe": "^4.8.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.23.0",
    "@faker-js/faker": "^8.1.0",
    "@jest/globals": "^29.7.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/luxon": "^3.3.2",
    "@types/node": "^20.5.1",
    "@types/node-cron": "^3.0.9",
    "@types/qr-image": "^3.2.7",
    "@types/qrcode": "^1.5.2",
    "@types/supertest": "^2.0.13",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.2.0",
    "prisma-dbml-generator": "^0.10.0",
    "supertest": "^6.3.3",
    "swagger-autogen": "^2.23.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.1",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6",
    "winston": "^3.10.0"
  }
}
